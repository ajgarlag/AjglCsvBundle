{
    "name": "ajgl/csv-bundle",
    "type": "symfony-bundle",
    "description": "Provides some CSV helper clases",
    "keywords": ["csv", "bundle", "symfony"],
    "homepage": "https://github.com/ajgarlag/AjglCsvBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Antonio J. GarcÃ­a Lagar",
            "email": "aj@garcialagar.es",
            "homepage": "http://aj.garcialagar.es",
            "role": "developer"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ajgl/csv": "^0.4",
        "symfony/framework-bundle": "^2.7|^3.4|^4.1"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.4|^4.1"
    },
    "autoload": {
        "psr-4": {"Ajgl\\Bundle\\CsvBundle\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"Ajgl\\Bundle\\CsvBundle\\Tests\\": "tests/"}
    },
    "scripts": {
        "phpunit": "vendor/bin/simple-phpunit --colors=always",
        "test": [
            "@phpunit"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.5.x-dev"
        }
    }
}
